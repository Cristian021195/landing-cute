---
import About from "../components/layout/About.astro";
import Chefs from "../components/layout/Chefs.astro";
import ContactLocation from "../components/layout/ContactLocation.astro";
import Gallery from "../components/layout/Gallery.astro";
import Header from "../components/layout/Header.astro";
import HeroMain from "../components/layout/HeroMain.astro";
import MenuList from "../components/layout/MenuList.astro";
import Reservation from "../components/layout/Reservation.astro";
import SpecialEvents from "../components/layout/SpecialEvents.astro";
import Specials from "../components/layout/Specials.astro";
import Testimonials from "../components/layout/Testimonials.astro";
import WhyUs from "../components/layout/WhyUs.astro";
import Head from "../components/meta/Head.astro";
import Footer from "../components/layout/Footer.astro";
import './../style.css';
---

<html lang="es">
	<Head title={'Romano Restobar'}/>
	<body>
		<Header/>
		<HeroMain/>
		<main id="main">
			<About/>
			<WhyUs/>
			<MenuList/>
			<Specials/>
			<SpecialEvents/> 
			<Reservation/>
			<Testimonials/>
			<Gallery/>
			<Chefs/>
			<ContactLocation/>
		</main>
		<Footer/>
		<div id="preloader"></div>
		<a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>	
		
	</body>
</html>
